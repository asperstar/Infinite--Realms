{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
    "functions": {
      "api/index.js": {
        "memory": 128,
        "maxDuration": 10
      }
    },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(favicon.ico|manifest.json|logo192.png|logo512.png|robots.txt)",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
       "dest": "/api/index.js"
    }
  ]
}